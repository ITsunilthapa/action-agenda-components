{
  "name": "@action-agenda/wcmc",
  "version": "2.0.0-beta.0",
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "DEV=true sdo build && dist-builder && DEV=true sdo toDist",
    "release": "yarn publish --access=public --tag beta --tag latest",
    "clean:reinstall": "if test -f yarn.lock; then rm yarn.lock; fi && if test -d node_modules; then rm -rf node_modules; fi && yarn --exact --force"
  },
  "dependencies": {
    "@action-agenda/default-config": "^2.0.0-beta.1",
    "@houlagins/schema-dot-organizer": "^1.0.1-beta.0",
    "core-js": "3.9.0",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@houlagins/dist-builder": "~1.0.11",
    "@vue/cli-plugin-babel": "4.5.11",
    "@vue/cli-plugin-e2e-cypress": "4.5.11",
    "@vue/cli-plugin-eslint": "4.5.11",
    "@vue/cli-service": "4.5.11",
    "babel-eslint": "10.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-vue": "7.6.0",
    "vue-template-compiler": "2.6.11"
  },
  "description": "https://github.com/scbd/action-agenda-network/tree/master/src/partners/wcmc#index",
  "readme": "https://github.com/scbd/action-agenda-network/tree/master/src/partners/wcmc#index", 
  "license": "MIT",
  "main": "dist/esm/index.mjs",
  "web": "dist/esm/index.mjs",
  "unpkg": "dist/esm/index.mjs",
  "jsdelivr": "dist/esm/index.mjs",
  "module": "dist/esm/index.min.mjs",
  "jsnext:main": "dist/esm/index.min.mjs",
  "src": "src/index.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "browser": {
    "./": "dist/esm/index.mjs"
  },
  "exports": {
    ".": [
      {
        "import": "./dist/esm/index.mjs",
        "default": "./dist/esm/index.mjs"
      }
    ]
  }
}
